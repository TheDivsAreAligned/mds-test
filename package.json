{
  "name": "mckinsey-coding-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "@latest",
    "@material-ui/icons": "@latest",
    "hookrouter": "^1.2.3",
    "jss-plugin-global": "^10.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^2.1.2",
    "react-svg": "^10.0.22",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --coverage --env=jsdom --collectCoverageFrom='src/App.test.js' --collectCoverageFrom='src/components/**' --collectCoverageFrom='src/shared/**'  --collectCoverageFrom='src/pages/**'",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint-plugin-react-hooks": "^0.0.0",
    "husky": "^1.3.1",
    "jest-enzyme": "^7.0.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.18.2",
    "react-testing-library": "^6.0.2"
  },
  "setupFiles": [
    "<rootDir>/src/setupTests.js"
  ],
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "hooks": {
    "pre-commit": "lint-staged"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "coverageReporters": [
      "text"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
